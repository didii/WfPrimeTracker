{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.8.1",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.woff2"
      ],
      "destination": "wwwroot/lib/font-awesome"
    },
    {
      "library": "jquery@3.4.0",
      "files": [
        "jquery.slim.min.js"
      ],
      "destination": "wwwroot/lib/jquery"
    },
    {
      "library": "popper.js@1.15.0",
      "files": [
        "umd/popper.min.js"
      ],
      "destination": "wwwroot/lib/popper.js"
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.min.js"
      ],
      "destination": "wwwroot/lib/twitter-bootstrap"
    }
  ]
}