{
    "name": "wf-prime-tracker",
    "version": "0.1.0",
    "main": "app.js",
    "license": "MIT",
    "author": "Danijel Hrcek",
    "directories": {
        "@/*": [
            "ClientApp/*"
        ]
    },
    "scripts": {
        "dev": "./node_modules/.bin/webpack --mode=development --watch --config webpack.config.js",
        "build:dev": "./node_modules/.bin/webpack --mode=development --config webpack.config.js",
        "build:prod": "./node_modules/.bin/webpack --mode=production --config webpack.config.js",
        "publish": "npm install && ./node_modules/.bin/webpack --mode=production --config webpack.config.js && dotnet publish --configuration Release",
        "test": "jest"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.8.1",
        "@types/aos": "^3.0.1",
        "@types/jquery": "3.3.29",
        "@types/node": "10.12.18",
        "@vue/test-utils": "1.0.0-beta.28",
        "@types/uuid": "^3.4.4",
        "acorn": "^6.1.1",
        "aos": "^2.3.4",
        "aspnet-webpack": "3.0.0",
        "axios": "0.18.0",
        "babel-core": "6.26.3",
        "babel-jest": "23.6.0",
        "babel-loader": "7.1.5",
        "babel-plugin-dynamic-import-node": "2.2.0",
        "babel-plugin-syntax-dynamic-import": "6.18.0",
        "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
        "babel-plugin-transform-export-extensions": "6.22.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-stage-2": "6.24.1",
        "bootstrap": "^4.3.1",
        "clean-webpack-plugin": "1.0.0",
        "compression-webpack-plugin": "2.0.0",
        "css-loader": "^2.1.1",
        "es6-promise-promise": "1.0.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "3.0.1",
        "html-webpack-plugin": "3.2.0",
        "jest": "23.6.0",
        "jest-serializer-vue": "2.0.2",
        "jquery": "^3.3.1",
        "node-sass": "4.11.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "popper.js": "^1.15.0",
        "resolve-url-loader": "3.0.0",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "ts-loader": "5.3.3",
        "typescript": "3.2.4",
        "url-loader": "1.1.2",
        "uuid": "^3.3.2",
        "vue": "2.5.22",
        "vue-class-component": "6.3.2",
        "vue-flatpickr-component": "^8.1.2",
        "vue-jest": "^3.0.4",
        "vue-lazyload": "^1.2.6",
        "vue-loader": "15.5.1",
        "vue-multiselect": "^2.1.4",
        "vue-notification": "^1.3.16",
        "vue-property-decorator": "7.3.0",
        "vue-router": "^3.0.3",
        "vue-style-loader": "4.1.2",
        "vue-template-compiler": "2.5.22",
        "vuex": "3.1.0",
        "vuex-module-decorators": "^0.9.8",
        "webpack": "^4.29.6",
        "webpack-cli": "3.2.1",
        "webpack-dev-server": "3.1.14",
        "webpack-hot-middleware": "2.24.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/ClientApp/$1"
        },
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "<rootDir>/ClientApp/**/*.{js,vue}",
            "!**/node_modules/**"
        ],
        "coverageDirectory": "<rootDir>/ClientApp/__tests__/TestCoverage",
        "coverageThreshold": {
            "global": {
                "branches": 10,
                "functions": 10,
                "lines": 10,
                "statements": -10
            }
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
